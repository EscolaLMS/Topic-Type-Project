{
    "name": "escolalms/topic-type-project",
    "description": "Escola Headless LMS Topic Type Project",
    "type": "package",
    "require": {
        "php": ">=7.4",
        "escolalms/auth": "^0",
        "escolalms/core": "^1",
        "laravel/framework": ">=8.0",
        "escolalms/topic-types": "^0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": ">=5.0",
        "darkaonline/l5-swagger": "8.1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Maciej Rymarz",
            "email": "maciej.rymarz@escolasoft.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "EscolaLms\\TopicTypeProject\\": "src",
            "EscolaLms\\TopicTypeProject\\Tests\\": "tests",
            "EscolaLms\\TopicTypeProject\\Database\\Factories\\": "database/factories",
            "EscolaLms\\TopicTypeProject\\Database\\Seeders\\": "database/seeders"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EscolaLms\\TopicTypeProject\\EscolaLmsTopicTypeProjectServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": false,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
